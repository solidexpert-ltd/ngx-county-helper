{
  "name": "@solidexpert/ngx-country-helper",
  "version": "15.0.0",
  "description": "Angular helper to get coyntry by timezone.",
  "repository": {
    "type": "git",
    "url": "https://github.com/solidexpert-ltd/ngx-county-helper.git"
  },
  "homepage": "https://solidexpert.ltd",
  "bugs": {
    "url": "https://github.com/solidexpert-ltd/ngx-county-helper"
  },
  "files": [
    "lib_commonjs/*",
    "lib_esmodule/*",
    "README.md",
    "LICENSE"
  ],
  "author": "Siarhei Shchypanau",
  "license": "MIT",
  "keywords": [
    "angular",
    "ng",
    "timezone",
    "country",
    "handler"
  ],
  "main": "./lib_commonjs/index.js",
  "module": "./lib_esmodule/index.js",
  "jsnext:main": "./lib_esmodule/index.js",
  "typings": "./lib_commonjs/index.d.ts",
  "scripts": {
    "lint": "tslint --project tsconfig.json src/**/*.ts",
    "build:commonjs": "rimraf lib_commonjs && ngc -p tsconfig.json",
    "build:esmodule": "rimraf lib_esmodule && ngc -p tsconfig.module.json",
    "build": "npm run lint && npm run build:commonjs && npm run build:esmodule",
    "prepare": "npm run build"
  },
  "peerDependencies": {
  },
  "devDependencies": {
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "tslint": "~6.1.3",
    "typescript": "^4.8.2",
    "zone.js": "~0.11.4"
  }
}
